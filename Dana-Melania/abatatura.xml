			<entry>
				<form type="headword">
					<orth n="">abătătură</orth>
					<stress xml:lang="ro-x-accent-upcase-vowels">ABĂTĂTÚRĂ</stress>
					<syll/>
					<pron/>
				</form>
				<gramGrp>
					<pos value="s."/>
					<gen value="f."/>
					<number value=""/>
					<case value=""/>
