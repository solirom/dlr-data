						<sense xml:id="uuid-0b1f2a36-a6b5-4fef-bd91-30366c792f22" n="✧">
							<re type="unitate-semantică-subsumată" subtype="syntagma">
								<usg>
									<seg type="dom" rend="usg">GEOGR.</seg>
								</usg>
								
								<term>Groapă abisală</term>
							</re>
							<re type="unitate-semantică-subsumată" subtype="unknown">
								<usg>
									<seg type="dom" rend="usg">GEOGR.</seg>
								</usg>
								
								<term>Fosă abisală</term>
							</re>
							<def xml:id="id">ormă majoră a reliefului fundului oceanelor, cu aspect
								de groapă sau de şanţ, ale cărei adâncimi depăşesc 6 000 m.</def>
							<cit type="unknown">
