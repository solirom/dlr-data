					<orth n="">abilitate</orth>
					<stress xml:lang="ro-x-accent-upcase-vowels">ABILITÁTE</stress>
					<syll/>
					<pron/>
				</form>
				<gramGrp>
					<pos value="s."/>
					<gen value="f."/>
					<number value=""/>
