						<def xml:id="uuid-d82c8ff1-a8e4-43bb-96ed-121eaa936cfa">(Corp, substanţă
							etc.) care absoarbe lichide sau gaze;</def>
						<xr type="syn" corresp="#uuid-d82c8ff1-a8e4-43bb-96ed-121eaa936cfa">
							<ptr target="absorbitor (1)"/>
						</xr>
						<xr type="syn" corresp="#uuid-d82c8ff1-a8e4-43bb-96ed-121eaa936cfa">
