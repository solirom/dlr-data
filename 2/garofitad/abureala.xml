			<entry>
				<form type="headword">
					<orth n="">abureală</orth>
					<stress xml:lang="ro-x-accent-upcase-vowels">ABUREÁLĂ</stress>
					<syll/>
					<pron/>
				</form>
				<gramGrp>
					<pos value="s."/>
					<gen value="f."/>
					<number value=""/>
					<case value=""/>
