			<entry>
				<form type="headword">
					<orth n="">abandon</orth>
					<stress xml:lang="ro-x-accent-upcase-vowels">ABANDÓN</stress>
					<syll/>
					<pron/>
				</form>
				<gramGrp>
					<pos value="s."/>
					<gen value="n."/>
					<number value=""/>
