				<dictScrap xml:id="dlr-senses">
					<sense xml:id="uuid-62d59002-3729-4fc7-9ccc-ace899312dc0" n="1.">
						<sense xml:id="uuid-809d6d55-d4c4-4501-893c-c1016e4094a1" n="✧">
							<def xml:id="id"/>
							<usg corresp="#id">(<seg type="hint" rend="usg">Ca termen de comparație,
									sugerează ideea de duritate, de asprime</seg>)</usg>
							<cit type="unknown">
								<quote>M-a îmbătrânit, mi-a scos peri albi, Și ea, Șade tot <hi rend="bold">abanos</hi>, țapăn și vârtos.</quote>
								<bibl type="unknown">
									<ptr target="#A104374">pann</ptr>
									<date>1847</date>
									<citedRange>78</citedRange>
								</bibl>
							</cit>
						</sense>
						<def xml:id="uuid-7913f7c7-730b-4f16-b4d4-a400da53be51">Arbore exotic, din
							familia <hi rend="italic">Ebenaceae</hi>, cu lemnul negru și dur, din
							zonele tropicale și subtropicale (mai ales din India) </def>
						<usg corresp="#uuid-7913f7c7-730b-4f16-b4d4-a400da53be51">
							<seg type="dom" rend="usg">BOT.</seg>
						</usg>
						<term xml:lang="la" type="unknown" subtype="unknown">Diospyros ebenum</term>
						<cit type="unknown">
							<quote>Acolo [în Etiopia] iaste aur mult și pili și copacii toți
								sălbatici și <hi rend="bold">abanos</hi>. </quote>
							<bibl type="unknown">
								<ptr target="#A100918">istorii</ptr>
								<date>&lt;1664-1668&gt;</date>
								<citedRange>176</citedRange>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote>Într-acest ostrov să face mult chiperiu și <hi rend="bold">abanos</hi>.</quote>
							<bibl type="unknown">
								<ptr target="#A103567">hotiniu</ptr>
								<date>1795</date>
								<citedRange>148/7</citedRange>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote>Pe strâmte căi, prin crâng de <hi rend="bold">abanos</hi>.</quote>
							<bibl type="unknown">
								<ptr target="#A104529">cump</ptr>
								<date>&lt;1941-1944&gt;</date>
								<citedRange>339</citedRange>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote>Lemnul are o gamă plăcută de culori...Variază în ce privește
								duritatea de la fragilitatea lemnului de balsa și de tei până la
								duritatea aproape de piatră a <hi rend="bold">abanosului</hi>.</quote>
							<bibl type="unknown">
								<ptr target="#A104554">gridan</ptr>
								<date>2003</date>
								<citedRange>150</citedRange>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote>Verde foaie <hi rend="bold">abanos</hi>, Rămâi taică
								sănătos.</quote>
							<bibl type="unknown">
								<ptr target="#A102805">folc.
									do</ptr>
								<date>1887</date>
								<citedRange>498</citedRange>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote>Frunzuliță d-<hi rend="bold">abanos</hi>, Trece sara pe din
								dos.</quote>
							<bibl type="unknown">
								<ptr target="#A101297">m.
									fol</ptr>
								<date>ante 1897</date>
								<citedRange>235</citedRange>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote>Frundză verdi di-<hi rend="bold">abănos</hi>.</quote>
							<bibl type="unknown">
								<ptr target="#A100316">vasiliu</ptr>
								<date>1897</date>
								<citedRange>109</citedRange>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote>În fundul salei era o estradă încunjurată cu o balustradă de lemn
								de <hi rend="bold">abanos</hi>.</quote>
							<bibl type="unknown">
								<ptr target="#A103055">leon</ptr>
								<date>1821</date>
								<citedRange>30/1</citedRange>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote/>
							<bibl type="unknown">
								<ptr target="#A104011">asachi</ptr>
								<date>1842</date>
								<citedRange/>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote>În fundul salei era o stradă ceva mai înaltă, ocolită cu grile de
								lemn de <hi rend="bold">abanos</hi>.</quote>
							<bibl type="unknown">
								<ptr target="#A104381">coliba</ptr>
								<date>1850</date>
								<citedRange>17</citedRange>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote/>
							<bibl type="unknown">
								<ptr target="#A101803">pont</ptr>
								<date>1862</date>
								<citedRange/>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote/>
							<bibl type="unknown">
								<ptr target="#A105061">barcia</ptr>
								<date>1868</date>
								<citedRange/>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote/>
							<bibl type="unknown">
								<ptr target="#A104144">costinescu</ptr>
								<date>1870</date>
								<citedRange/>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote/>
							<bibl type="unknown">
								<ptr target="#A102406">lmd</ptr>
								<date>1871</date>
								<citedRange/>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote/>
							<bibl type="unknown">
								<ptr target="#A101394">șăinea</ptr>
								<date>1896</date>
								<citedRange/>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote>Avea două bufete vaste de lemn de <hi rend="bold">abanos</hi>.</quote>
							<bibl type="unknown">
								<ptr target="#A101089">galaction</ptr>
								<date>1832</date>
								<citedRange>47</citedRange>
							</bibl>
						</cit>
					</sense>
					<sense xml:id="uuid-a0404aab-f8cf-4771-be9b-b7113fdb3815" n="2.">
						<sense xml:id="uuid-9cb95ad0-1091-4682-b472-74d483200836" n="✧">
							<def xml:id="id"/>
							<usg corresp="#id">
								<seg type="sem" rend="usg">Fig.</seg>
							</usg>
							<cit type="unknown">
								<quote>Noaptea viind triumfătoare, Cu steag negru-ntunecos... Stând
									în tron de <hi rend="bold">abanos</hi>.</quote>
								<bibl type="unknown">
									<ptr target="#A105047">mumulean</ptr>
									<date>1937</date>
									<citedRange>222</citedRange>
								</bibl>
							</cit>
						</sense>
						<sense xml:id="uuid-756008a5-e364-4be6-95c8-0e1215c260bf" n="✧">
							<re type="unitate-semantică-subsumată">
								<idno n="tip-unitate-semantică-subsumată" type="adjectival-locution"/>
								<term>De abanos</term>
							</re>
							<def xml:id="id">foarte negru, negru-lucios. </def>
							<cit type="unknown">
								<quote>Câteva grece cu părul de <hi rend="bold">abanos</hi> și cu
									ochii negri, ce se plimbau dinaintea bisericei … fură marture
									nepăsătoare acestei întâmplări ciudate.</quote>
								<bibl type="unknown">
									<ptr target="#A100783">ralet</ptr>
									<date>1858</date>
									<citedRange>111</citedRange>
								</bibl>
							</cit>
							<cit type="unknown">
								<quote>Mariţa începu să-i împletească părul cel de <hi rend="bold">abanos</hi>.</quote>
								<bibl type="unknown">
									<ptr target="#A101871">gane</ptr>
									<date>1870</date>
									<citedRange>73</citedRange>
								</bibl>
							</cit>
							<cit type="unknown">
								<quote>În bogăția părului cu reflexe de <hi rend="bold">abanos</hi>
									strălucea beteala.</quote>
								<bibl type="unknown">
									<ptr target="#A101752">conv.
										lit</ptr>
									<date>1920</date>
									<citedRange>LII, nr. 7, 398</citedRange>
								</bibl>
							</cit>
							<cit type="unknown">
								<quote>Lumina acestei amiezi, semn al unei toamne aurii, îl
									înconjoară ca un reflector, scoţând şi mai puternic în evidenţă
									părul de <hi rend="bold">abanos</hi> al omului şi trăsăturile
									aspre, regulate, ale feţei străjuită de o mustaţă cât o pată de
									tuş. </quote>
								<bibl type="unknown">
									<ptr target="#A105067">viața
										ro</ptr>
									<date>1954</date>
									<citedRange>martie, nr. 3, 230</citedRange>
								</bibl>
							</cit>
						</sense>
						<def xml:id="id">Lemnul acestui arbore, folosit pentru fabricarea mobilei de
							lux, a instru¬mentelor muzicale de suflat etc.</def>
						<cit type="unknown">
							<quote>1 săcrei viarde; 1 săcreiaș de <hi rend="bold">abanos</hi> și
								poleit.</quote>
							<bibl type="ap.">
								<ptr target="#A102873">hem</ptr>
								<date>1669</date>
								<citedRange>83</citedRange>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote>Au scos dintr-însa o tablă mică de trei picioare de lată şi de
								doaă de lungă, de <hi rend="bold">abanos</hi>.</quote>
							<bibl type="unknown">
								<ptr target="#A102519">ceasornicul</ptr>
								<date>&lt;1703-1709&gt;</date>
								<citedRange>677</citedRange>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote>Locuia pe liniştita stradă a Modei, la catul al doilea al unei
								clădiri ce aparţinea regelui Carol, la o franţuzoaică bătrână care-i
								închiriase două încăperi, bogat mobilate în gustul greoi de acum
								cincizeci de ani. La belșugul de <hi rend="bold">abanos</hi> și de
								mahon … adăoga o nebunească risipă de trandafiri.</quote>
							<bibl type="unknown">
								<ptr target="#A101592">craii</ptr>
								<date>1929</date>
								<citedRange>65</citedRange>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote>Egiptenii … aducea … <hi rend="bold">abanos</hi> …
								aromate.</quote>
							<bibl type="unknown">
								<ptr target="#A102575">genilie</ptr>
								<date>1835</date>
								<citedRange>10/18</citedRange>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote>Mobilierele cele căptușite cu <hi rend="bold">abanos</hi>.</quote>
							<bibl type="unknown">
								<ptr target="#A101973">indiana</ptr>
								<date>1847</date>
								<citedRange>5/25</citedRange>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote>Cioplan purta... în mână un toiag de <hi rend="bold">abanos</hi>.</quote>
							<bibl type="unknown">
								<ptr target="#A104059">asachi</ptr>
								<date>1853</date>
								<citedRange>213</citedRange>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote>Aș bufni în râsul cel mai veninos să văd un trubadur-modern: cu
								inimă imensă... cu baston de <hi rend="bold">abanos</hi>.</quote>
							<bibl type="unknown">
								<ptr target="#A100305">delavran</ptr>
								<date>1881</date>
								<citedRange>346</citedRange>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote>Orele unsprezece sunau la pendula de <hi rend="bold">abanos</hi>.</quote>
							<bibl type="unknown">
								<ptr target="#A102711">macedons</ptr>
								<date>1892</date>
								<citedRange>86</citedRange>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote>M-am uitat cu atenție la pălăria lui moale, la jacheta bine
								croită, la bastonul său de <hi rend="bold">abanos</hi>.</quote>
							<bibl type="unknown">
								<ptr target="#A102213">patrașcanu</ptr>
								<date>1909</date>
								<citedRange>81</citedRange>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote>Acele mătănii erau făcute din materiile cele mai diverse: lemn de
								odagaciu, chiparos,... <hi rend="bold">abanos</hi>.</quote>
							<bibl type="unknown">
								<ptr target="#A101286">r.
									rose</ptr>
								<date>1922</date>
								<citedRange>407</citedRange>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote>Își lărgise eșarfa de mătase și-și sprijinea trupul într-un șarpe
								înțelept de <hi rend="bold">abanos</hi>.</quote>
							<bibl type="unknown">
								<ptr target="#A103794">blocada</ptr>
								<date>1947</date>
								<citedRange>84</citedRange>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote>Așezat la măsuța de <hi rend="bold">abanos</hi>... Câmpineanu se
								pregătea să scrie.</quote>
							<bibl type="unknown">
								<ptr target="#A104719">un om</ptr>
								<date>1953</date>
								<citedRange>693</citedRange>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote>Te duc în patul biblic de cedru și-<hi rend="bold">abanos</hi>.</quote>
							<bibl type="unknown">
								<ptr target="#A100749">lauda</ptr>
								<date>1963</date>
								<citedRange>126</citedRange>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote>Se juca cu nește mătănii de <hi rend="bold">abanos</hi>.</quote>
							<bibl type="unknown">
								<ptr target="#A103240">săptăm</ptr>
								<date>1981</date>
								<citedRange>181</citedRange>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote>Am văzut cum, sub ochii mei chiar, se deschidea capacul unei
								ascunzători... pe fundul căreia lucea întunecat <hi rend="bold">abanosul</hi> celei mai frumoase casete orientale. </quote>
							<bibl type="unknown">
								<ptr target="#A101084">calpuz</ptr>
								<date>1987</date>
								<citedRange>7</citedRange>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote>Faraonul înfăţişat pe o frescă este aşezat lângă un scaun de <hi rend="bold">abanos</hi>.</quote>
							<bibl type="unknown">
								<ptr target="#A104554">gridan</ptr>
								<date>2003</date>
								<citedRange>30</citedRange>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote>Egiptenii … se neguțătoria pe Marea Roșie ... și aducea avoriu,
									<hi rend="bold">abanos</hi>.</quote>
							<bibl type="unknown">
								<ptr target="#A102575">genilie</ptr>
								<date>1835</date>
								<citedRange>10/18</citedRange>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote/>
							<bibl type="unknown">
								<ptr target="#A101803">pont</ptr>
								<date>1862</date>
								<citedRange/>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote/>
							<bibl type="unknown">
								<ptr target="#A104144">costinescu</ptr>
								<date>1870</date>
								<citedRange/>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote/>
							<bibl type="unknown">
								<ptr target="#A102406">lmd</ptr>
								<date>1871</date>
								<citedRange/>
							</bibl>
						</cit>
					</sense>
