        <form type="headword">
          <orth n="">abataj</orth>
          <stress xml:lang="ro-x-accent-upcase-vowels">ABATÁJ</stress>
          <syll/>
          <pron/>
        </form>
        <gramGrp>
          <pos value="s."/>
          <gen value="n."/>
