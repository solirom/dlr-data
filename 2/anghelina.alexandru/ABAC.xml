				<form type="headword">
					<orth n="">abac</orth>
					<stress xml:lang="ro-x-accent-upcase-vowels">ABÁC </stress>
					<syll/>
					<pron/>
				</form>
				<gramGrp>
					<pos value="s."/>
					<gen value="n."/>
					<number value=""/>
					<case value=""/>
