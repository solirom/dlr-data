				<form type="headword">
					<orth n="">abager</orth>
					<stress xml:lang="ro-x-accent-upcase-vowels">ABAGÉR </stress>
					<syll/>
					<pron/>
				</form>
				<gramGrp>
					<pos value="s."/>
					<gen value="m."/>
					<number value=""/>
					<case value=""/>
