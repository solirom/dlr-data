				<form type="headword">
					<orth n="1">abagerie</orth>
					<stress xml:lang="ro-x-accent-upcase-vowels">ABAGERÍE</stress>
					<syll/>
					<pron/>
				</form>
				<gramGrp>
					<pos value="s."/>
					<gen value="f."/>
					<number value=""/>
					<case value=""/>
