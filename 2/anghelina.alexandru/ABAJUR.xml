					<orth n="">abajur</orth>
					<stress xml:lang="ro-x-accent-upcase-vowels">ABAJÚR</stress>
					<syll/>
					<pron/>
				</form>
				<gramGrp>
					<pos value="s."/>
					<gen value="n."/>
					<number value=""/>
