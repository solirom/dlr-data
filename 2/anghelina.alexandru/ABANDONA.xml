				<dictScrap xml:id="dlr-senses">
					<sense xml:id="uuid-16b8782f-31c9-46cf-8e8b-b0a7e19c5dfd" n="1.">
						<sense xml:id="uuid-9e177822-be05-45b6-b92e-d40183b44e77" n="✧">
							<def xml:id="id"/>
							<usg corresp="#id">(<seg type="gram" rend="usg">Cu pronumele în
									dativ</seg>)</usg>
							<cit type="unknown">
								<quote>D. Carp... <hi rend="bold">şi-a abandonat</hi> tablele de
									legi.</quote>
								<bibl type="unknown">
									<ptr target="#uuid-bf779331-7a1f-491d-1ee3-ed783e022073">discursuri</ptr>
									<date>1894</date>
									<citedRange>14</citedRange>
								</bibl>
							</cit>
							<cit type="unknown">
								<quote>Eugenița <hi rend="bold">își abandonă</hi> păpușa într-un
									colț umbrit.</quote>
								<bibl type="unknown">
									<ptr target="#uuid-98763bcd-5fe6-982a-5a49-e414a9fc3650">eugeni</ptr>
									<date>1936</date>
									<citedRange>303</citedRange>
								</bibl>
							</cit>
						</sense>
						<sense xml:id="uuid-779b9fc4-9a04-4d6e-af20-0bbd1811fb9f" n="✧">
							<gramGrp>
								<pos value="vb."/>
								<iType value="I"/>
								<subc value="refl. pas."/>
								<subc value=""/>
							</gramGrp>
							<def xml:id="id"/>
							<cit type="unknown">
								<quote>Proprietățile ce <hi rend="bold">se abandon</hi> de
									proprietari sunt lipsite de capitalurile rulante necesarie
									pentru a face ca întreprinderea să poată merge înainte.</quote>
								<bibl type="unknown">
									<ptr target="#uuid-6f8ed817-3166-84f4-0691-79fc5e5c46ae">MO</ptr>
									<date>1878</date>
									<citedRange>1 038</citedRange>
								</bibl>
							</cit>
							<cit type="unknown">
								<quote><hi rend="bold">Se abandonează</hi> proiectul ... şi pietrele
									rămân expuse pe unde se poate, cum sunt şi până azi.</quote>
								<bibl type="unknown">
									<ptr target="#uuid-95d94add-5c45-0e9a-a603-33c0acf72709">arh.
										ol</ptr>
									<date>1938</date>
									<citedRange>XVII, 174</citedRange>
								</bibl>
							</cit>
							<cit type="unknown">
								<quote>E ciudat însă că <hi rend="bold">se abandonează</hi> şi
									oraşe, ca Kalinin (Tver) în care trupele ar fi putut găsi un
									adăpost.</quote>
								<bibl type="unknown">
									<ptr target="#uuid-0c21e14f-4671-8e87-09e7-8c6275419cfd">argetoianu</ptr>
									<date>1941</date>
									<citedRange>527</citedRange>
								</bibl>
							</cit>
						</sense>
						<sense xml:id="uuid-2a6373ce-381a-49e7-841c-0821a906b355" n="✦">
							<sense xml:id="uuid-f6972b1e-458c-474b-9f6f-218531c3a8b0" n="✧">
								<gramGrp>
									<pos value="vb."/>
									<iType value="I"/>
									<subc value="refl. pas."/>
									<subc value=""/>
								</gramGrp>
								<def xml:id="id"/>
								<cit type="unknown">
									<quote>I <hi rend="bold">se abandonau</hi> copiii în faţa
										prăvăliei, uneori şi de femei cu care nu avusese prea mult
										de-a face.</quote>
									<bibl type="unknown">
										<ptr target="#uuid-c2d5d543-6ba1-67e2-4764-d37fca0fa0dc">bănulescu</ptr>
										<date>1977</date>
										<citedRange>377</citedRange>
									</bibl>
								</cit>
								<etym cert="high">
									<idno type=""/>
								</etym>
							</sense>
							<def xml:id="id">A părăsi, lăsând fără sprijin sau ajutor:</def>
							<usg corresp="#id">(<seg type="gram" rend="usg">Complementul indică
									copii, familie, persoane asistate</seg>)</usg>
							<xr type="syn" corresp="#id">
								<ptr target="a lepăda (II 1)"/>
								<usg><seg type="time" rend="usg">înv.</seg> și <seg type="register style" rend="usg">pop.</seg></usg>
							</xr>
							<cit type="unknown">
								<quote>Iată omul fuge de locu-i natal, <hi rend="bold">Abandonă</hi>
									mumă, tată, soră, frate, Lasă-n voia soartei copii, amici,
									toate.</quote>
								<bibl type="unknown">
									<ptr target="#uuid-db5e8b65-a6f2-468c-774d-d15328324fff">aricescu</ptr>
									<date>1847</date>
									<citedRange>83/13</citedRange>
								</bibl>
							</cit>
							<cit type="unknown">
								<quote>Voiai <hi rend="bold">să abandoni</hi> pe bătrânul tău
									părinte.</quote>
								<bibl type="unknown">
									<ptr target="#uuid-751d3d72-4748-5f4a-33ab-d3ec96c52732">filimon</ptr>
									<date>1860</date>
									<citedRange>251</citedRange>
								</bibl>
							</cit>
							<cit type="unknown">
								<quote>Copil <hi rend="bold">abandonat</hi> de mama sa, hoț de
									timpuriu … el este un orgolios.</quote>
								<bibl type="unknown">
									<ptr target="#uuid-552be625-6d3e-797a-0ce6-94b850d87d45">viața
										ro</ptr>
									<date>1963</date>
									<citedRange>septembrie, nr. 9, 164</citedRange>
								</bibl>
							</cit>
							<cit type="unknown">
								<quote>Individa în cauză – că mamă nu se mai poate numi – <hi rend="bold">a abandonat </hi>fetiţa pe marginea
									drumului.</quote>
								<bibl type="unknown">
									<ptr target="#uuid-405f68e4-1a5c-268d-6468-8b28a28da1ea">adevărul</ptr>
									<date>1996</date>
									<citedRange>nr. 1 786, 3</citedRange>
								</bibl>
							</cit>
							<cit type="unknown">
								<quote>Un copil <hi rend="bold">abandonat</hi> într-un coșuleț roz,
									scâncind ca un pui de pisică, și la gât cu un medalion de
									preț.</quote>
								<bibl type="unknown">
									<ptr target="#uuid-a6f144b5-551a-497b-10a7-f962efe19f23">adame</ptr>
									<date>1997 (1983)</date>
									<citedRange>124</citedRange>
								</bibl>
							</cit>
							<cit type="unknown">
								<quote/>
								<bibl type="unknown">
									<ptr target="#uuid-e6f0f4d6-1654-205b-2004-26f4c2832fb4">pake</ptr>
									<date>1862</date>
									<citedRange/>
								</bibl>
							</cit>
						</sense>
