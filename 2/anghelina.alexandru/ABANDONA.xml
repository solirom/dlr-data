							<usg corresp="#uuid-dc45cffe-bcf0-4f2e-ad37-59a0c592fdbd">
								<seg type="sem" rend="usg">Fig.</seg>
							</usg>
							<cit type="unknown">
								<quote>Picioarele-i subțiri să cadă <hi rend="bold">abandonate</hi>
									peste bara mușchiuloasă a brațului său drept.</quote>
								<bibl type="unknown">
									<ptr target="#A103987">nunții</ptr>
									<date>1933</date>
									<citedRange>21</citedRange>
								</bibl>
							</cit>
						</sense>
						<gramGrp>
							<pos value="vb."/>
