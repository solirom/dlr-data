                        <sense xml:id="uuid-6a3febff-0467-433e-bb82-69ebabf66765" n="1.">
                            <sense xml:id="uuid-c135c97c-b5ba-4524-99b8-fe1f0a111634" n="◊">
                                <def xml:id="uuid-05db7d4a-18ec-4b5c-b343-355177c062a5"/>
                                <usg corresp="#uuid-05db7d4a-18ec-4b5c-b343-355177c062a5">
                                    <seg type="sem" rend="usg">Fig.</seg>
                                </usg>
                                <cit type="unknown">
                                    <quote>De ce te dai lenei, d-aia mai mult ea te coprinde, ş-apoi
                                        tot ce este rău este foarte lipicios.</quote>
                                    <bibl type="unknown">
                                        <ptr target="#A100816"/>
                                        <date/>
                                        <citedRange>18</citedRange>
                                    </bibl>
                                </cit>
                            </sense>
                            <sense xml:id="uuid-662cfaca-be74-4c99-a929-61270913a6b3" n="◊">
                                <def xml:id="uuid-c11a1861-fc1e-40ce-a1bd-e048e6652d33"/>
                                <usg corresp="#uuid-c11a1861-fc1e-40ce-a1bd-e048e6652d33">
                                    <seg type="gram" rend="usg">(Adverbial)</seg>
                                </usg>
                                <cit type="unknown">
                                    <quote>Cea mai infimă creatură este o revelare a unei infinite
                                        puteri, a unei incomensurabile şi eterne voinţe, fie acel
                                        fenomen o râmă oarbă, care se târăşte lipicios pe un palmac
                                        de mocirlă în umbră.</quote>
                                    <bibl type="unknown">
                                        <ptr target="CARAGIALE, O. III"/>
                                        <date/>
                                        <citedRange>57</citedRange>
                                    </bibl>
                                </cit>
                            </sense>
                            <sense xml:id="uuid-6c6f5276-e998-4467-b569-416d40e0dc20" n="♦">
                                <def xml:id="uuid-556163ba-c47d-445c-8b78-1eff4b605bbe"> Urduros.</def>
                                <usg corresp="#uuid-556163ba-c47d-445c-8b78-1eff4b605bbe">
                                    <seg type="time" rend="usg">învechit</seg>
                                    <seg type="plev" rend="usg">rar</seg>
                                </usg>
                                <cit type="unknown">
                                    <quote/>
                                    <bibl type="cf.">
                                        <ptr target="#A104676"/>
                                        <date/>
                                        <citedRange>305</citedRange>
                                    </bibl>
                                </cit>
                            </sense>
                            <sense xml:id="uuid-dafab347-f20c-41af-9511-68d775cff320" n="◊">
                                <def xml:id="uuid-38fbf16c-7fb8-4411-a9b8-330629541edc">Compus : lipicios-deolaltă = glutinos.</def>
                                <usg corresp="#uuid-38fbf16c-7fb8-4411-a9b8-330629541edc">
                                    <seg type="time" rend="usg">învechit</seg>
                                </usg>
                                <cit type="unknown">
                                    <quote>Semânţele cele glutinoase (lipicioase-deolaltă).</quote>
                                    <bibl type="unknown">
                                        <ptr target="#A101910"/>
                                        <date/>
                                        <citedRange>121/1</citedRange>
                                    </bibl>
                                </cit>
                            </sense>
                            <gramGrp>
                                <pos value=""/>
                            </gramGrp>
                            <def xml:id="uuid-78ecb5f9-b322-43cb-a0d4-5dbe38825c31"> Care se lipeşte (13); care aderă la obiectele cu care vine în
                                atingere, c l eios, v â s cos; care are la exterior o materie
                                vâscoasă (ca cleiul).</def>
                            <usg corresp="#uuid-78ecb5f9-b322-43cb-a0d4-5dbe38825c31">
                                <seg type="hint" rend="usg">Despre substanţe, materii etc.</seg>
                                <seg type="hint" rend="usg">Despre corpuri sau părţi ale lor</seg>
                            </usg>
                            <cit type="unknown">
                                <quote>Tocmi... o unsoare de smoală cu său şi cu de alte lipicioase
                                    şi unsuroase şi ardzoase lucruri.</quote>
                                <bibl type="unknown">
                                    <ptr target="Dosoftei, V. S."/>
                                    <date/>
                                    <citedRange>noiembrie 128r/7</citedRange>
                                </bibl>
                            </cit>
                        </sense>
