					<orth n="">ab intestat</orth>
					<stress xml:lang="ro-x-accent-upcase-vowels">AB INTESTÁT </stress>
					<syll/>
					<pron/>
				</form>
				<gramGrp>
					<pos value="loc."/>
					<subc value="adj."/>
				</gramGrp>
				<gramGrp>
					<pos value="loc."/>
