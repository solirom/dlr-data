					<orth n="">abilitare</orth>
					<stress xml:lang="ro-x-accent-upcase-vowels">ABILITÁRE </stress>
					<syll/>
					<pron/>
				</form>
				<gramGrp>
					<pos value="s."/>
					<gen value="f."/>
					<number value=""/>
					<case value=""/>
