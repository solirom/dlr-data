				<form type="headword">
					<orth n="">abdicare</orth>
					<stress xml:lang="ro-x-accent-upcase-vowels">ABDICÁRE</stress>
					<syll/>
					<pron/>
				</form>
				<gramGrp>
					<pos value="s."/>
					<gen value="f."/>
					<number value=""/>
