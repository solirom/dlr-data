					<orth n="1">abate</orth>
					<stress xml:lang="ro-x-accent-upcase-vowels">ABÁTE</stress>
					<syll/>
					<pron/>
				</form>
				<gramGrp>
					<pos value="s."/>
					<gen value="m."/>
					<number value=""/>
