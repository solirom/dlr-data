			<entry>
				<form type="headword">
					<orth n="">abatere</orth>
					<stress xml:lang="ro-x-accent-upcase-vowels">ABÁTERE</stress>
					<syll/>
					<pron/>
				</form>
				<gramGrp>
					<pos value="s."/>
