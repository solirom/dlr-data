			<entry>
				<form type="headword">
					<orth n="">abia</orth>
					<stress xml:lang="ro-x-accent-upcase-vowels">ABIÁ</stress>
					<syll/>
					<pron/>
				</form>
				<gramGrp>
					<pos value="adv."/>
