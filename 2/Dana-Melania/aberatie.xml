			<entry>
				<form type="headword">
					<orth n="">aberatie</orth>
					<stress xml:lang="ro-x-accent-upcase-vowels">ABERÁŢIE</stress>
					<syll/>
					<pron/>
				</form>
				<gramGrp>
					<pos value="s."/>
					<gen value="f."/>
					<number value=""/>
					<case value=""/>
