			<entry>
				<form type="headword">
					<orth n="">abces</orth>
					<stress xml:lang="ro-x-accent-upcase-vowels">ABCÉS </stress>
					<syll/>
					<pron/>
				</form>
				<gramGrp>
					<pos value="s."/>
