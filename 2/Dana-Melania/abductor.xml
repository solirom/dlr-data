				<dictScrap xml:id="dlr-senses">
					<sense xml:id="uuid-1d6e602d-3c1f-43b3-b50a-562621d65f3d" n="">
						<sense xml:id="uuid-a3935932-290f-4606-b643-3b102cd3b85c" n="">
							<re type="unitate-semantică-subsumată">
								<usg>
									<seg type="dom" rend="usg">ANAT.</seg>
								</usg>
								<idno n="tip-unitate-semantică-subsumată" type="syntagma"/>
								<term>Muşchi abductor</term>
							</re>
							<def xml:id="id">muşchi care permite, care realizează mișcarea de
								abducție.</def>
							<cit type="unknown">
								<quote>Artira plantera cea internă, se dirigează pe marginea de din
									afară a muşchiului <hi rend="bold">abductoru</hi> al degetului
									celui mare.</quote>
								<bibl type="unknown">
									<ptr target="#A103303">anat</ptr>
									<date>1843</date>
									<citedRange>401</citedRange>
								</bibl>
							</cit>
							<cit type="unknown">
								<quote>M[ușchiul] <hi rend="bold">abductorul</hi> lung al degetului
									mare resare de pă fața ecsternă a antebrațului și de pă
									ligam[entul] interosos și se prinde de basa metacarpienului
									degetului mare.</quote>
								<bibl type="unknown">
									<ptr target="#A101349">poliz</ptr>
									<date>1859</date>
									<citedRange>65</citedRange>
								</bibl>
							</cit>
							<cit type="unknown">
								<quote>Muşchi aductori şi <hi rend="bold">abductori</hi> deschid şi
									închid cochilia.</quote>
								<bibl type="unknown">
									<ptr target="#A100171">agr</ptr>
									<date>1937</date>
									<citedRange>487</citedRange>
								</bibl>
							</cit>
						</sense>
						<sense xml:id="uuid-3c75337b-806c-4844-a716-cff1f3bd5ab9" n="">
							<re type="unitate-semantică-subsumată">
								<idno n="tip-unitate-semantică-subsumată" type="syntagma"/>
								<term>Tub abductor</term>
							</re>
							<def xml:id="id">tub prin intermediul căruia pot fi absorbite gazele
								rezultate dintr-o reacţie chimică.</def>
							<usg corresp="#id">
								<seg type="dom" rend="usg">TEHN.</seg>
							</usg>
							<cit type="unknown">
								<quote>Se introduce ... un tub <hi rend="bold">abductor</hi>.</quote>
								<bibl type="unknown">
									<ptr target="#A100415">arh</ptr>
									<date>1890</date>
									<citedRange>II, 146</citedRange>
								</bibl>
							</cit>
						</sense>
						<sense xml:id="uuid-d509b65b-8663-4283-943b-fcb061dbebb2" n="✧">
							<def xml:id="id"/>
							<usg corresp="#id">(<seg type="gram" rend="usg">Substantivat</seg>)</usg>
							<cit type="unknown">
								<quote>Aceşti muşchi ... se împart în muşchii între-osoşii cei
									dorsali şi cei palmeri. În fiecare spaţiu se află câte doi, unul
									dorsal şi altul palmer, prin urmare ar trebui să fie opt, cel
									dintâiu însă palmer s-a descris subt numirea de <hi rend="bold">abducătorul</hi> degetului celui mare.</quote>
								<bibl type="unknown">
									<ptr target="#A103303">kret</ptr>
									<date>1843</date>
									<citedRange>235</citedRange>
								</bibl>
							</cit>
						</sense>
						<def xml:id="uuid-53ca414e-8b14-436b-ac98-94382b08c7a7">Care servește la
							abducție.</def>
						<usg corresp="#uuid-53ca414e-8b14-436b-ac98-94382b08c7a7">
