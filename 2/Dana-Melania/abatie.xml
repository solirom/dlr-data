					<orth n="">abație</orth>
					<stress xml:lang="ro-x-accent-upcase-vowels">ABAŢÍE </stress>
					<syll/>
					<pron/>
				</form>
				<gramGrp>
					<pos value="s."/>
					<gen value="f."/>
					<number value=""/>
