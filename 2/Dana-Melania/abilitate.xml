						<cit type="unknown">
							<quote/>
							<bibl type="unknown">
								<ptr target="#uuid-d82be227-360e-8412-6d1e-d6497fc598ed">psihiat</ptr>
								<date>1987</date>
								<citedRange/>
							</bibl>
