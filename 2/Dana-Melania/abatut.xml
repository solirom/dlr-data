						</xr>
						<xr type="syn" corresp="#id">
							<ptr target="idiot"/>
						</xr>
						<cit type="unknown">
							<quote>Unul eră mai isteţ... celalalt eră mai mutălău, mai <hi rend="bold">abătut</hi> cu leuca.</quote>
							<bibl type="unknown">
								<ptr target="#uuid-f658a78e-241d-60e5-07a2-e574df7d29d4">cătană</ptr>
								<date>1893</date>
								<citedRange>130</citedRange>
							</bibl>
						</cit>
					</sense>
