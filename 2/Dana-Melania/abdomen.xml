				<dictScrap xml:id="dlr-senses">
					<sense xml:id="uuid-49280d68-a530-45ab-abc2-80b48b9bb198" n="1.">
						<sense xml:id="uuid-dbf0d06a-83c7-4b9d-b973-67d9ddd8b4d7" n="✧">
							<def xml:id="id"/>
							<usg corresp="#id">
								<seg type="sem" rend="usg">Fig.</seg>
							</usg>
							<cit type="unknown">
								<quote>E un fel de trecătoare de infern, prin care te strecori cu
									pălăria în mână, ... cerându-ţi iertare de la o soţie lovită-n
									genunchi sau în osul bărbiei, subt ochiul aspru al soţului
									spânzurat într-un guler sau plutitor pe un <hi rend="bold">abdomen</hi>.</quote>
								<bibl type="unknown">
									<ptr target="#A100959">cortin</ptr>
									<date>1913</date>
									<citedRange>97</citedRange>
								</bibl>
							</cit>
							<cit type="unknown">
								<quote>Puţini vor putea spune că pricep mai bine, dar multe <hi rend="bold">abdomene</hi> dogmatice vor rămâne în
									aer.</quote>
								<bibl type="unknown">
									<ptr target="#A102432">fenom</ptr>
									<date>1936</date>
									<citedRange>446</citedRange>
								</bibl>
							</cit>
						</sense>
						<sense xml:id="uuid-725bc846-ead5-45c0-b9bd-d7c43915bed9" n="">
							<re type="unitate-semantică-subsumată">
								<usg>
									<seg type="dom" rend="usg">MED.</seg>
								</usg>
								<idno n="tip-unitate-semantică-subsumată" type="syntagma"/>
								<term>Abdomen acut</term>
							</re>
							<def xml:id="id">sindrom abdominal cu debut brusc, caracterizat prin
								durere, contractură musculară, temperatură ridicată, greţuri,
								vărsături, cauzat de inflamaţia, perforaţia sau obstrucţia etc. unui
								organ intern și care impune adesea intervenţia chirurgicală de
								urgenţă</def>
							<cit type="unknown">
								<quote/>
								<bibl type="unknown">
									<ptr target="#A102606">ABC</ptr>
									<date>1964</date>
									<citedRange/>
								</bibl>
							</cit>
							<cit type="unknown">
								<quote/>
								<bibl type="unknown">
									<ptr target="#A100818">medic</ptr>
									<date>1969</date>
									<citedRange/>
								</bibl>
							</cit>
						</sense>
						<sense xml:id="uuid-67a2a174-30a7-4bc4-b812-93a6002fe988" n="✧">
							<usg corresp="#uuid-67a2a174-30a7-4bc4-b812-93a6002fe988">
								<seg type="dom" rend="usg">MED.</seg>
							</usg>
							<re type="unitate-semantică-subsumată">
								<idno n="tip-unitate-semantică-subsumată" type="syntagma"/>
								<term>Abdomen de broască</term>
							</re>
							<re type="unitate-semantică-subsumată">
								<usg>
									<seg type="dom" rend="usg">MED.</seg>
								</usg>
								<idno n="tip-unitate-semantică-subsumată" type="syntagma"/>
								<term>Abdomen de batracian</term>
							</re>
							<def xml:id="id">aspect particular al abdomenului la bolnavii cu ascită
								veche, caracterizat prin lărgirea părților laterale ale acestuia,
								care devin asemănătoare ca formă cu abdomenul broaștelor.</def>
							<cit type="unknown">
								<quote/>
								<bibl type="unknown">
									<ptr target="#A102606">ABC</ptr>
									<date>1964</date>
									<citedRange/>
								</bibl>
							</cit>
							<cit type="unknown">
								<quote/>
								<bibl type="unknown">
									<ptr target="#A102325">medic</ptr>
									<date>2007</date>
									<citedRange/>
								</bibl>
							</cit>
						</sense>
						<sense xml:id="uuid-3df71b6c-2cb7-42f5-852d-9e9c24a40a45" n="✧">
							<re type="unitate-semantică-subsumată">
								<usg>
									<seg type="dom" rend="usg">MED.</seg>
								</usg>
								<idno n="tip-unitate-semantică-subsumată" type="syntagma"/>
								<term>Abdomen în corabie</term>
							</re>
							<def xml:id="id">aspect particular al abdomenului, a cărui parte
								centrală este excavată, formând o depresiune delimitată de ultimele
								coaste şi de oasele iliace, întâlnit în numeroase boli, mai ales în
								holeră, meningită tuberculoasă, colică saturnină şi în stări de
								emaciere.</def>
							<cit type="unknown">
								<quote/>
								<bibl type="unknown">
									<ptr target="#A102325">medic</ptr>
									<date>2007</date>
									<citedRange/>
								</bibl>
							</cit>
						</sense>
						<sense xml:id="uuid-5a91ac98-b028-4ba7-b990-6e744a223983" n="✦">
							<def xml:id="id">Exerciţiu fizic care constă în utilizarea intensă a
								muşchilor abdominali, prin apropierea repetată a toracelui de
								coapse.</def>
							<usg corresp="#id">(<seg type="plev" rend="usg">Mai ales la
								pl.</seg>)</usg>
							<cit type="unknown">
								<quote/>
								<bibl type="unknown">
									<ptr target="#A103590">dicr</ptr>
									<date>2011</date>
									<citedRange/>
								</bibl>
							</cit>
						</sense>
						<def xml:id="uuid-41ca1f97-0441-468e-b0be-74203441370d">Regiune a corpului
							vertebratelor, situată între torace şi pelvis, în care se află stomacul,
							ficatul, pancreasul, splina, rinichii şi intestinele;</def>
						<usg corresp="#uuid-41ca1f97-0441-468e-b0be-74203441370d">
							<seg type="dom" rend="usg">ANAT.</seg>
						</usg>
						<xr type="syn" corresp="#uuid-41ca1f97-0441-468e-b0be-74203441370d">
							<ptr target="burtă"/>
						</xr>
						<xr type="syn" corresp="#uuid-41ca1f97-0441-468e-b0be-74203441370d">
							<ptr target="pântece"/>
							<usg>
								<seg type="register style" rend="usg">pop.</seg>
							</usg>
						</xr>
						<xr type="syn" corresp="#uuid-41ca1f97-0441-468e-b0be-74203441370d">
							<ptr target="foale"/>
							<usg>
								<seg type="geo" rend="usg">reg.</seg>
							</usg>
						</xr>
						<cit type="unknown">
							<quote/>
							<bibl type="unknown">
								<ptr target="#A100539">corbea</ptr>
								<date/>
								<citedRange/>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote>Linia albă a <hi rend="bold">abdomenului</hi> nu este dar alt
								decât o încrucişare la partea din mijloc a aponevrozelor
								muşchilor.</quote>
							<bibl type="unknown">
								<ptr target="#A103303">kretz</ptr>
								<date>1843</date>
								<citedRange>167/21</citedRange>
							</bibl>
						</cit>
						<cit type="unknown">CIPARIU<quote>Ştii d-ta ce simţ io şi trebuie să simţă
								oricine care cât de puţin ş-au studiat limba istoriceşte? Greaţă!
								Abandonament! Abandonare! etc. <hi rend="bold">abdomen</hi>,
								abdominal etc., abordabil! … D[omnul] Eliad le dă vorbe de origine
								romană şi necesarii la curăţirea (?!) şi cultura (!) limbei române!!
								Ferească-ne Dumnezeu!</quote><bibl type="ap.">
								<ptr target="#A100444">contemp</ptr>
								<date>1847</date>
								<citedRange>272</citedRange>
							</bibl></cit>
						<cit type="unknown">
							<quote>Era o femeie, încă tânără, care murise de foame, judecând după
								înfăţişarea ei groaznic de sărăcăcioasă şi după depresiunea <hi rend="bold">abdomenului</hi>.</quote>
							<bibl type="unknown">
								<ptr target="#A102691">varia</ptr>
								<date>1919</date>
								<citedRange>336</citedRange>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote><hi rend="bold">Abdomineul</hi> are pelea belită şi ... îngăurită
								ca de armă de vânătoare.</quote>
							<bibl type="unknown">
								<ptr target="#A100240">carnet</ptr>
								<date>&lt;1927–1938&gt;</date>
								<citedRange>33</citedRange>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote>Sybill ... s-a întors la Bucureşti, chinuită de un ciudat fenomen
								de umflătură în regiunea <hi rend="bold">abdomenului</hi>.</quote>
							<bibl type="unknown">
								<ptr target="#A100742">gafen</ptr>
								<date>1931</date>
								<citedRange>275</citedRange>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote>Apucat în Orient Expres de dureri nebune de <hi rend="bold">abdomen</hi>, de năduşeli şi de otihneli, a fost debarcat la
								Viena, pe o targă.</quote>
							<bibl type="unknown">
								<ptr target="#A101386">arget</ptr>
								<date>1936</date>
								<citedRange>458</citedRange>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote>Accidentatul este culcat pe <hi rend="bold">abdomen</hi> (pe
								burtă) pe o suprafaţă orizontală.</quote>
							<bibl type="unknown">
								<ptr target="#A100192">belea</ptr>
								<date>1959</date>
								<citedRange>83</citedRange>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote>Femela … cedează icrele la uşoara apăsare a <hi rend="bold">abdomenului</hi>.</quote>
							<bibl type="unknown">
								<ptr target="#A101445">vânăt</ptr>
								<date>1961</date>
								<citedRange>iulie, 6</citedRange>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote/>
							<bibl type="unknown">
								<ptr target="#A100603">zoot</ptr>
								<date>1969</date>
								<citedRange/>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote/>
							<bibl type="unknown">
								<ptr target="#A103012">zoot</ptr>
								<date>1982</date>
								<citedRange/>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote>Aplicare de cataplasme calde stimulative pe <hi rend="bold">abdomenul</hi> suferinzilor [de holeră].</quote>
							<bibl type="unknown">
								<ptr target="#A100419">cernov</ptr>
								<date>2002</date>
								<citedRange>61</citedRange>
							</bibl>
						</cit>
					</sense>
					<sense xml:id="uuid-f0e3e1c0-c524-4406-9126-a9760de47a0f" n="2.">
						<def xml:id="id">Partea posterioară a corpului la artropode.</def>
						<usg corresp="#id">
							<seg type="dom" rend="usg">BIOL.</seg>
						</usg>
						<cit type="unknown">
							<quote>Sunt animale hexapode (cu 6 picioare) având corpul lor trei
								despărţituri bine destinate, adică un cap, un toraci (pept) şi un
									<hi rend="bold">abdomen</hi> (burtă).</quote>
							<bibl type="unknown">
								<ptr target="#A101387">barasc</ptr>
								<date>1861</date>
								<citedRange>237</citedRange>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote>După cefalotorace urmează <hi rend="bold">abdomenul</hi>.</quote>
							<bibl type="unknown">
								<ptr target="#A104582">zool</ptr>
								<date>1962</date>
								<citedRange>56</citedRange>
							</bibl>
						</cit>
						<cit type="unknown">
							<quote>Într-un loc cu iarbă mai rară, mergeau grăbite două mici gângănii
								roşii, cu pete negre pe aripi şi cu picioare subţiri, lucioase şi
								negre, legate între ele prin extremitatea <hi rend="bold">abdomenului</hi>.</quote>
							<bibl type="unknown">
								<ptr target="#A104183">iliesc</ptr>
								<date>1970</date>
								<citedRange>227</citedRange>
							</bibl>
						</cit>
					</sense>
