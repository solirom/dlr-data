				<form type="headword">
					<orth n="">abdomen</orth>
					<stress xml:lang="ro-x-accent-upcase-vowels">ABDOMÉN</stress>
					<syll/>
					<pron/>
				</form>
				<gramGrp>
					<pos value="s."/>
					<gen value="n."/>
					<number value=""/>
