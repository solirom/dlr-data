                        <sense xml:id="uuid-8fa1d68e-84f8-48df-9c9a-59604348cc40" n="◊">
                            <def xml:id="uuid-3a51a154-45c3-4b7d-8f0a-f6f7476a2353"/>
                            <usg corresp="#uuid-3a51a154-45c3-4b7d-8f0a-f6f7476a2353">
