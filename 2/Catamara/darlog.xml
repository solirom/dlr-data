                            <def xml:id="uuid-1029098c-b757-4eb8-8b97-b0e2729572fa">Femeie decăzută, de moravuri uşoare,</def>
                            <xr type="syn" corresp="#uuid-1029098c-b757-4eb8-8b97-b0e2729572fa">
                                <ptr target="prostituată."/>
