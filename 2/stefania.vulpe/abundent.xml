				<form type="headword">
					<orth n="">abundent</orth>
					<stress xml:lang="ro-x-accent-upcase-vowels">ABUNDÉNT</stress>
					<syll/>
					<pron/>
				</form>
				<gramGrp>
					<pos value="adj."/>
