					<orth n=""/>
					<stress xml:lang="ro-x-accent-upcase-vowels">FÁRBĂ</stress>
					<syll/>
					<pron/>
				</form>
				<gramGrp>
					<pos value="s."/>
					<gen value="f."/>
					<number value=""/>
					<case value=""/>
