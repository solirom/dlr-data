                                <form type="unitate-semantică-subsumată">
                                    <idno n="tip-unitate-semantică-subsumată" type="phrase"/>
                                    <term>În starea darului/ A fi în
                                    starea darului</term>
                                </form>
                                <def xml:id="uuid-14bac4c0-5676-455a-806e-368e2ac77230">(a fi) gravidă</def>
