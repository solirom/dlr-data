                                <form type="unitate-semantică-subsumată">
                                    <idno n="tip-unitate-semantică-subsumată" type="phrase"/>
                                    <term>A avea darul să .../ A avea
                                    darul de a .../ A avea darul de...</term>
                                </form>
                                <def xml:id="uuid-ba5f32dd-9647-4218-a6f2-a0dc1dd02688">a avea puterea, capacitatea, priceperea, calitatea,
                                    efectul să ...</def>
                                <def xml:id="uuid-a92651fe-6a79-4edb-8d81-99f849794a8f">a fi în stare de a...</def>
                                <cit type="unknown">
                                    <quote>A avea darul de a înţelege limba dobitoacelor</quote>
