                        <sense xml:id="uuid-b95f5b88-25b9-441f-b895-becd786c4fdd" n="4.">
                            <def xml:id="uuid-c482ef72-4ca9-459b-b8d9-5b1189d5f80c">Răsplată;</def>
                            <usg corresp="#uuid-c482ef72-4ca9-459b-b8d9-5b1189d5f80c">
