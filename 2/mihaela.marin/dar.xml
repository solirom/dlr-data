                            </usg>
                            <def xml:id="uuid-006be0d7-a81d-49b6-b850-3a53b69a3db7">premiu (2)</def>
                            <usg corresp="#uuid-006be0d7-a81d-49b6-b850-3a53b69a3db7">
                                <seg type="time" rend="usg">înv.</seg>
                            </usg>
                            <cit type="unknown">
                                <quote>Vor vedea slava svinţilor şi darurele minunată şi frâmseaţe
                                    ... dulceaţa şi bunătatea</quote>
                                <bibl type="unknown">
                                    <ptr target="#uuid-eb954596-8779-2651-80d9-2f1fd96f61eb"/>
                                    <date/>
                                    <citedRange>222</citedRange>
                                </bibl>
                            </cit>
                        </sense>
                        <sense xml:id="uuid-702bef10-cd90-4e3d-94e9-b5e213a94d79" n="5.">
                            <sense xml:id="uuid-f9d1dade-ea39-471f-8230-8bec14b52bc5" n="◊">
                                <form type="unitate-semantică-subsumată">
                                    <idno n="tip-unitate-semantică-subsumată" type="phrase"/>
                                    <term>A face darul cuiva</term>
                                </form>
                                <def xml:id="uuid-6c182243-fdeb-4b04-8725-d824c89ed8ff">a face o favoare cuiva</def>
                                <usg corresp="#uuid-6c182243-fdeb-4b04-8725-d824c89ed8ff">
                                    <seg type="register style" rend="usg">ironic</seg>
                                </usg>
                                <cit type="unknown">
                                    <quote>Îmi face darul de a-şi lua drepturi asupra mea.</quote>
                                    <bibl type="unknown">
                                        <ptr target="#uuid-eb5c1090-6c27-5b17-88de-db6834366433"/>
                                        <date/>
                                        <citedRange>161</citedRange>
                                    </bibl>
                                </cit>
                            </sense>
                            <def xml:id="uuid-08372c2b-e028-414e-a081-73c4fc62ac9a">Folos, avantaj;</def>
                            <def xml:id="uuid-2d7fe413-b955-43db-ad3c-f97f036b34f9">favoare,</def>
                            <xr type="syn" corresp="#uuid-2d7fe413-b955-43db-ad3c-f97f036b34f9">
                                <ptr target="privilegiu"/>
                            </xr>
                            <cit type="unknown">
                                <quote>Cela ce va fi scutealnic să nu plătească vamă, de vreme ce
                                    i-au dat Dumnedzău un dar ca acesta.</quote>
                                <bibl type="unknown">
                                    <ptr target="#uuid-88488b46-97fd-9888-72f8-e1566f803d25"/>
                                    <date/>
                                    <citedRange>49</citedRange>
                                </bibl>
