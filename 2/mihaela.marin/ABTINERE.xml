			<entry>
				<form type="headword">
					<orth n="">abţinere</orth>
					<stress xml:lang="ro-x-accent-upcase-vowels">ABŢÍNERE</stress>
					<syll/>
					<pron/>
				</form>
				<gramGrp>
					<pos value="s."/>
					<gen value="f."/>
