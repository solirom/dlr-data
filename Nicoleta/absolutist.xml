			<entry>
				<form type="headword">
					<orth n="">absolutist</orth>
					<stress xml:lang="ro-x-accent-upcase-vowels">ABSOLUTÍST</stress>
					<syll/>
					<pron/>
				</form>
				<gramGrp>
					<pos value="s."/>
					<gen value="m. si f."/>
					<number value=""/>
					<case value=""/>
