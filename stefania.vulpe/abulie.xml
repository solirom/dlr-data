					<orth n="">abulie</orth>
					<stress xml:lang="ro-x-accent-upcase-vowels">ABULÍE</stress>
					<syll/>
					<pron/>
				</form>
				<gramGrp>
					<pos value="s."/>
					<gen value="f."/>
					<number value=""/>
