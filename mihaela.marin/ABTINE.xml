			<entry>
				<form type="headword">
					<orth n="">abţine</orth>
					<stress xml:lang="ro-x-accent-upcase-vowels">ABŢÍNE</stress>
					<syll/>
					<pron/>
				</form>
				<gramGrp>
					<pos value="vb."/>
					<iType value="III"/>
					<subc value=""/>
					<subc value=""/>
